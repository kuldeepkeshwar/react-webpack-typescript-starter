{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "node": "current"
      }
    }],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel","transform-class-properties"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": ["env", "react"]
    }
  }
}